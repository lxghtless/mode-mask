{
    "name": "mode-mask",
    "version": "2.2.0",
    "description": "Manage mode and permissions with masks/flags",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lxghtless/mode-mask.git"
    },
    "author": "lxghtless",
    "license": "ISC",
    "url": "https://github.com/lxghtless/mode-mask",
    "homepage": "https://github.com/lxghtless/mode-mask",
    "keywords": [
        "mode",
        "permission",
        "mask"
    ],
    "scripts": {
