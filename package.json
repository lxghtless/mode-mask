        "@types/node": "^14.14.6",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "intern": "^4.8.8",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
